<?xml version="1.0" encoding="utf-8"?>

<ModMetaData>
    <name>Simple Trans</name>
    <description>This mod adds a simple system for differentiating how pawns interact with pregnancy and AGAR (Assigned Gender at Rimworld).</description>
    <author>Runaway</author>
    <supportedVersions>
        <li>1.5</li>
        <li>1.6</li>
    </supportedVersions>
    <packageId>runaway.simpletrans</packageId>
    <modDependencies>
        <li>
            <packageId>imranfish.xmlextensions</packageId>
            <displayName>XML Extensions</displayName>
            <downloadUrl>https://github.com/15adhami/XmlExtensions/releases</downloadUrl>
            <steamWorkshopUrl>steam://url/CommunityFilePage/2574315206</steamWorkshopUrl>
        </li>
        <li>
            <packageId>brrainz.harmony</packageId>
            <displayName>Harmony</displayName>
            <downloadUrl>https://github.com/pardeike/HarmonyRimWorld/releases/latest</downloadUrl>
            <steamWorkshopUrl>https://steamcommunity.com/workshop/filedetails/?id=2009463077</steamWorkshopUrl>
        </li>
        <li>
            <packageId>OskarPotocki.VanillaFactionsExpanded.Core</packageId>
            <displayName>Vanilla Expanded Framework</displayName>
            <steamWorkshopUrl>https://steamcommunity.com/workshop/filedetails/?id=2023507013</steamWorkshopUrl>
            <downloadUrl>https://github.com/Vanilla-Expanded/VanillaExpandedFramework</downloadUrl>
        </li>
    </modDependencies>
    <loadAfter>
        <li>imranfish.xmlextensions</li>
        <li>Ludeon.RimWorld.Biotech</li>
        <li>brrainz.harmony</li>
        <li>vanillaexpanded.vanillasocialinteractionsexpanded</li>
    </loadAfter>
    <incompatibleWith>
        <li>rim.job.world</li>
    </incompatibleWith>
</ModMetaData>